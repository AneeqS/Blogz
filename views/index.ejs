<% include ./partials/header %>

<% blogs.forEach(function (blog){ %>
    <div>

        <h2> <%= blog.title %></h2>
        <img src="<%= blog.image %>">
        <span><%= blog.created %></span>
        <p> <%= blog.content %></p>
        <a href="/blogs/<%= blogs._id%>">Read More!</a>

    </div>
<% })%>


<% include ./partials/footer %>
